#include <stdio.h>
#include <ctype.h>
#include <stdlib.h>
/**
 * checkint - check if it is integer
 * @str: Argument count
 * Return: 0 for succesful exit
 */
int checkint(char *str)
{
int i = 0;
while (str[i] != '\0')
{
if (!isdigit(str[i]))
{
return (0);
}
i++;
}
return (1);
}
/**
 * main - Print the name of the executable file
 * @argc: Argument count
 * @argv: Array of argument strings
 *
 * Return: 0 for succesful exit
 */
int main(int argc, char *argv[])
{
  int sum = 0, i;
  if (argc == 1)
    printf("%d\n", 0);
  else
    {
      if (checkint(*argv[])
	{
  for (i = 1; i < argc; i++)
    {
     sum += atoi(argv[i]);
    }
  printf("%d\n", sum);
	}
	else
	  {
	    printf("%s\n", "Error");
	    return (1);
	  }
	}
  return (0);
}
